{
	"Business Object": {
		"prefix": "tp-bo",
		"body": [
			"import { BaseBO, BODependencies, ApiResponse } from '../../src/core/business-objects/index.js'",
			"import { ${1:Name}Repository, ${1:Name}Service, ${1:Name}Messages, create${1:Name}Schemas, Schemas } from './${1:Name}Module.js'",
			"",
			"export class ${1:Name}BO extends BaseBO {",
			"    private service: ${1:Name}Service",
			"",
			"    constructor(deps: BODependencies) {",
			"        super(deps)",
			"        const repo = new ${1:Name}Repository(this.db)",
			"        this.service = new ${1:Name}Service(repo, this.log, this.config, this.db)",
			"    }",
			"",
			"    private get ${1/(.*)/${1:/downcase}/}Messages() {",
			"        return this.i18n.use(${1:Name}Messages)",
			"    }",
			"",
			"    private get ${1/(.*)/${1:/downcase}/}Schemas() {",
			"        return create${1:Name}Schemas(this.${1/(.*)/${1:/downcase}/}Messages)",
			"    }",
			"",
			"    async ${2:methodName}(params: Schemas.${3:InputType}): Promise<ApiResponse> {",
			"        return this.exec<Schemas.${3:InputType}, ${4:ReturnType}>(params, this.${1/(.*)/${1:/downcase}/}Schemas.${2:methodName}, async (data) => {",
			"            const result = await this.service.${2:methodName}(data)",
			"            return this.success(result, this.${1/(.*)/${1:/downcase}/}Messages.${5:successMessage})",
			"        })",
			"    }",
			"}"
		],
		"description": "ToProccess Business Object with 9-file structure and i18n support"
	},
	"BO Method (Transaction)": {
